@model List<CodingEventsDemo.Models.EventCategory>

<h1>Coding Categories</h1>


    <table class="table">
        <tr>
            <th>
                Category Name
            </th>            
        </tr>
        @foreach (EventCategory category in Model)
        {
            <tr>
                <td>@category.Name</td>
            </tr>
        }
    </table>
}